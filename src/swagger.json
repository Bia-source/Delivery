                                "schema": {
                                    "type": "object",
                                    "properties": {
                                                   "client": {
                                                       "type": "object",
                                                       "properties": {
                                                           "id": {
                                                               "type": "string"
                                                           },
                                                           "username": {
                                                               "type": "string"
                                                           },
                                                           "email": {
